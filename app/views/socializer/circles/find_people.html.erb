<%= render 'menu' %>
<ul class="large-contact-list">
<% Socializer::Person.all.each do |contact| %>
<li class="draggable"><%= link_to image_tag(contact.avatar_url, :width => 40, :height => 40), stream_path(:provider => :people, :id => contact.id), :class => "draggable", "data-object-id" => contact.guid, :title => contact.display_name %>
<% end %>
</ul>
<%= render 'circles' %>