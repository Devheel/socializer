<%= render 'menu' %>
<ul class="large-contact-list">
<% Socializer::Person.all.each do |contact| %>
<li><%= link_to image_tag(contact.avatar_url), stream_path(:provider => :people, :id => contact.id), :class => "draggable", "data-object-id" => contact.id, :title => contact.display_name %>
<% end %>
</ul>
