<%= form_for(person_phone) do |form| %>
<p class="field-title"><%= form.label :label %></p>               <p class="field-value"><%= form.text_field :label %></p>
<p class="field-title"><%= form.label :number %></p>              <p class="field-value"><%= form.text_field :number %></p>

<p class="field-title"></p>                                       <p class="field-value"><%= form.submit t('.submit'), class: "action-button" %></p>
<% end %>
