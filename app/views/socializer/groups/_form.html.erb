<% content_for :toolbar do %>
  <%= render 'toolbar' %>
<% end %>

<%= render 'menu' %>

<%= simple_form_for(group) do |f| %>
  <%= f.input :name, input_html: { autofocus: true } %>
  <%= f.input :privacy_level %>

  <%= f.button :submit, t('.submit'), class: 'btn-primary action-button' %>
<% end %>
